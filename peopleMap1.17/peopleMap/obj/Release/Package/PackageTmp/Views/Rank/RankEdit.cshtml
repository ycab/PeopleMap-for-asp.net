    <script type="text/javascript">
        $("button[data-up='']").on('click', function () {
            var input = $(this).parent().siblings("input");
            var num = input.val();
            num++;
            input.val(num);
        });
        $("button[data-down='']").on('click', function () {
            var input = $(this).parent().siblings("input");
            var num = input.val();
            if (num > 0) {
                num--;
            }
            else {
                alert("不可小于0！");
            }
            input.val(num);
        });
        $("#btn-submit").on('click', function () {//修改权重
            var ajaxParm = {};
            ajaxParm.ZCQZ = $("#ZCQZ").val();
            ajaxParm.YS = $("#YS").val();
            ajaxParm.JS = $("#JS").val();
            ajaxParm.FJS = $("#FJS").val();
            ajaxParm.YJY = $("#YJY").val();
            ajaxParm.QT = $("#QT").val();

            ajaxParm.TXQZ = $("#TXQZ").val();
            ajaxParm.YS1 = $("#YS1").val();
            ajaxParm.CJXZ = $("#CJXZ").val();
            ajaxParm.QRJH = $("#QRJH").val();
            ajaxParm.JCQN = $("#JCQN").val();
            ajaxParm.QT1 = $("#QT1").val();

            ajaxParm.ZWQZ = $("#ZWQZ").val();
            ajaxParm.ZWJF = $("#ZWJF").val();

            ajaxParm.ZLQZ = $("#ZLQZ").val();
            ajaxParm.ZLJF = $("#ZLJF").val();

            var a = parseFloat(ajaxParm.ZCQZ);
            var b = parseFloat(ajaxParm.TXQZ);
            var c = parseFloat(ajaxParm.ZWQZ);
            var d = parseFloat(ajaxParm.ZLQZ);

            if ((a + b + c + d) < parseFloat(99.999) || (a + b + c + d) > parseFloat(100)) {
                alert("各项权重之和不为100！");
            }
            else {
                var url = '@Url.Content("~/Rank/qzEdit")';
                $.post(url, ajaxParm, function (response) {

                    alert("修改成功");

                })
            }

        });

